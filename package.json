{
  "name": "biblatex-csl-converter",
  "version": "0.11.0",
  "description": "a set of converters: biblatex => json, json => biblatex, json => CSL",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fiduswriter/biblatex-csl-converter.git"
  },
  "keywords": [
    "bibtex",
    "biblatex",
    "citations",
    "csl",
    "citeproc"
  ],
  "author": "Johannes Wilm",
  "contributors": [
    "Henrik Muehe",
    "Takuto Kojima <mail@takutokojima.com>",
    "Johannes Wilm <mail@johanneswilm.org>"
  ],
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/fiduswriter/biblatex-csl-converter/issues"
  },
  "homepage": "https://github.com/fiduswriter/biblatex-csl-converter#readme",
  "scripts": {
    "check_js": "jshint src/ demo/demo.js",
    "compile": "rimraf lib/* && babel src -d lib && browserify demo/demo.js -o demo/demo-transpiled.js -t babelify",
    "prepublish": "npm run check_js && npm run compile"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "rimraf": "^2.5.4",
    "babelify": "7.3.0",
    "browserify": "13.1.1",
    "jshint": "2.9.4"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jshintConfig": {
    "esversion": 6,
    "loopfunc": true,
    "asi": true,
    "multistr": true,
    "sub": true,
    "undef": true,
    "predef": [
      "window",
      "document",
      "console"
    ]
  }
}
